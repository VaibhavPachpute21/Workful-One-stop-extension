{
    "name": "Workful",
    "description": "One stop extension for all tasks!",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "notifications",
        "storage",
        "alarms"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon":{
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    
    "icons":{"16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"}
}